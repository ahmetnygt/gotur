mixin seatVertical(genderColor,genderColorStroke)
    svg(viewBox="0 0 100 100" width="3rem" height="3rem" style="max-width:100px; border:1px solid #ccc")
        rect(x="20" y="20" width="60" height="60" rx="10" ry="10" fill=genderColor stroke=genderColorStroke stroke-width="2")
        rect(x="75" y="30" width="10" height="40" rx="5" ry="5" fill=genderColor stroke=genderColorStroke stroke-width="2")
        rect(x="15" y="30" width="10" height="40" rx="5" ry="5" fill=genderColor stroke=genderColorStroke stroke-width="2")
        rect(x="20" y="60" width="60" height="20" rx="5" ry="5" fill=genderColor stroke=genderColorStroke stroke-width="2")
        rect(x="35" y="55" width="30" height="30" rx="5" ry="5" fill=genderColor stroke=genderColorStroke stroke-width="2")